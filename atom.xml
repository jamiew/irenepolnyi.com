---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Irene Polnyi</title>
 <link href="http://irenepolnyi.com/atom.xml" rel="self"/>
 <link href="http://irenepolnyi.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://irenepolnyi.com/</id>
 <author>
   <name>Irene Polnyi</name>
   <email>irenepolnyi@gmail.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://irenepolnyi.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://irenepolnyi.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
